{
  "extends": "airbnb",
  "rules": {
    "no-new": 0,
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "prefer-destructuring": ["error", {"object": false, "array": false}],
    "no-console": "off",
    "react/destructuring-assignment": "off",
    "no-undef": "off"
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ]
}
